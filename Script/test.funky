
do
(
	def("++" function(//#i
		set_copy("#i" add(get("#i") 1))
	))
	
	set_copy("f" 2)
	set_copy("i" 0)
	while(less(get("i") 10) do(
		set_copy("f" mult(get("f") 2))
		print(get("i") ": " get("f") "\n")
		get("++" "#i" get("i"))
	))
	
	print(sub(30 10) " " div(10 2) "\n")
	print(add(to_int("12") 2) "\n")
	print(add(to_float("3.14") 0.001592) "\n")
	
	if(or(true false) do(
		print("bruh\n")
		if(and(false true) do(
			print("no")
		) if(not(false) do(
			print("yes\n")
		)))
	))
	
	if(equal("123" to_string(123)) 
		print("yoooo!\n")
	)
	
	def("vec3" function(// #x #y #z
		set_copy(".vec" list)
		push_copy(get(".vec") get("#x") get("#y") get("#z"))
		return_copy(get(".vec"))
	))
	
	set_ref("v1" get("vec3" "#x" 1 "#y" 2 "#z" 3))
	print(get("v1"))
	
	
	def("object" function(// #name #pos #vel
		set_copy(".obj" map)
		push_copy(get(".obj") 
			"name" get("#name")
			"pos" get("#pos")
			"vel" get("#vel")
		)
		return_copy(get(".obj"))
	))
	
	set_ref("obj" get("object" 
		"#name" "Greg" 
		"#pos" get("vec3" "#x" 2 "#y" 4 "#z" -5)
		"#vel" get("vec3" "#x" -1.2 "#y" -4.2 "#z" 5.4)
	))
	
	print("\n" get("obj") "\n")
)

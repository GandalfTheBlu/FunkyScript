#log_expanded "output.funky"
#include "std_macros.funky"

do
(
	grid = list
	i = 1
	while([.i <= 10] do(
		col = list
		push_ref(.grid .col)
		j = 1
		while([.j <= 10] do(
			col << [.i * .j]
			j++
		))
		i++
	))
	
	for(col in grid)
	{
		print(.col "\n")
	}
	
	for(i in [1 to 10])
	{
		print(.i "\n")
	}
)
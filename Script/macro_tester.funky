#log_expanded "output.funky"
#include "std_macros.funky"

do
(
	#include "stack.funky"
	
	stack = :Stack()
	
	print("push to stack (exit to stop):\n")
	inp = ""
	while([.inp != "exit"] do(
		input(.inp)
		if([.inp != "exit"] do(
			stack->Push(.inp)
		))
	))
	
	print("stack:\n")
	while([stack->Count() > 0] do(
		print(stack->Pop() "\n")
	))
)
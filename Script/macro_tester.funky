#log_expanded "output.funky"
#include "std_macros.funky"

do
(
	#include "stack.funky"
	#include "queue.funky"
	
	stack r= :Stack();
	stack->Push(1)
	stack->Push(2)
	stack->Push(3)
	print(.stack "\n")
	
	while([stack->Count() > 0] do(
		print(stack->Pop() "\n")
	))
	
	queue r= :Queue();
	queue->Push(1)
	queue->Push(2)
	queue->Push(3)
	print(.queue "\n")
	
	while([queue->Count() > 0] do(
		print(queue->Pop() "\n")
	))
)